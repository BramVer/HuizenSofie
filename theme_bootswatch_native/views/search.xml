<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
	<!-- Adds all assets in Odoo -->
        <template id="assets_backend" name="theme_bootswatch_native" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
		         <!-- All links to CSS and JS files should be added in here. -->
		         <link rel="stylesheet" href="/theme_bootswatch_native/static/src/css/style.css"/>
            </xpath>
        </template>

         <template id="assets_backend" name="theme_bootswatch_native" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
		<!--These links will be called when loading your Odoo -->
                <script type="text/javascript" src="/theme_bootswatch_native/static/src/js/search.js"></script>
            </xpath>
        </template>
    </data>
</openerp>